{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "frontend",
  "sourceRoot": "src/frontend/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "src/frontend"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "src/frontend"
      },
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "src/frontend"
      },
      "cache": true
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test:watch",
        "cwd": "src/frontend"
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test:e2e",
        "cwd": "src/frontend"
      }
    },
    "coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run coverage",
        "cwd": "src/frontend"
      },
      "cache": true
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "src/frontend"
      },
      "cache": true
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint:fix",
        "cwd": "src/frontend"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run typecheck",
        "cwd": "src/frontend"
      },
      "cache": true
    },
    "quality": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run quality",
        "cwd": "src/frontend"
      },
      "dependsOn": ["lint", "typecheck"],
      "cache": true
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run format",
        "cwd": "src/frontend"
      }
    },
    "format:check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run format:check",
        "cwd": "src/frontend"
      },
      "cache": true
    }
  },
  "tags": ["type:app", "platform:web"]
}
